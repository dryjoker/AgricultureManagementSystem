
@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                土地地段資料維護(單筆匯入)
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-12">
                        <form role="form">
                            <div class="form-group row">
                                <div class="col-sm-6">
                                    <label for="inputFirstname">縣市</label>
                                    <input type="text" class="form-control" id="inputFirstname">
                                </div>
                                <div class="col-sm-6">
                                    <label for="inputLastname">鄉鎮</label>
                                    <input type="text" class="form-control" id="inputLastname">
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-6">
                                    <label for="inputFirstname">地段</label>
                                    <input type="text" class="form-control" id="inputFirstname">
                                </div>
                                <div class="col-sm-6">
                                    <label for="inputLastname">地號</label>
                                    <input type="text" class="form-control" id="inputLastname">
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-6">
                                    <label for="inputFirstname">所有權人姓名</label>
                                    <input type="text" class="form-control" id="inputFirstname">
                                </div>
                                <div class="col-sm-6">
                                    <label for="inputLastname">所有權人身分證號</label>
                                    <input type="text" class="form-control" id="inputLastname">
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-6">
                                    <label for="inputFirstname">國土面積</label>
                                    <input type="text" class="form-control" id="inputFirstname">
                                </div>
                                <div class="col-sm-6">
                                    <label for="inputLastname">權利面積</label>
                                    <input type="text" class="form-control" id="inputLastname">
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-6">
                                    <label for="inputFirstname">契作面積</label>
                                    <input type="text" class="form-control" id="inputFirstname">
                                </div>
                                <div class="col-sm-6">
                                    <label for="inputLastname">契作人</label>
                                    <input type="text" class="form-control" id="inputLastname">
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-sm-6">
                                    <label for="inputFirstname">代耕人</label>
                                    <input type="text" class="form-control" id="inputFirstname">
                                </div>
                                <div class="col-sm-6">
                                    <label for="inputLastname">備註</label>
                                    <input type="text" class="form-control" id="inputLastname">
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col col-12 col-sm-6 col-md-4 col-lg-2">
                                    <label for="inputFirstname">小Y/大N</label>
                                    <div class="radio">
                                        <label>
                                            <input type="radio" name="optionsRadios3" id="optionsRadios3_1" value="Y" checked>Y
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input type="radio" name="optionsRadios3" id="optionsRadios3_2" value="N">N
                                        </label>
                                    </div>
                                </div>
                                <div class="col col-12 col-sm-6 col-md-4 col-lg-2">
                                    <label for="inputFirstname">專區Y/N</label>
                                    <div class="radio">
                                        <label>
                                            <input type="radio" name="optionsRadios1" id="optionsRadios1_1" value="Y" checked>Y
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input type="radio" name="optionsRadios1" id="optionsRadios1_2" value="N">N
                                        </label>
                                    </div>
                                </div>
                                <div class="col col-12 col-sm-6 col-md-4 col-lg-2">
                                    <label for="inputLastname">履歷Y/N</label>
                                    <div class="radio">
                                        <label>
                                            <input type="radio" name="optionsRadios2" id="optionsRadios2_1" value="Y" checked>Y
                                        </label>
                                    </div>
                                    <div class="radio">
                                        <label>
                                            <input type="radio" name="optionsRadios2" id="optionsRadios2_2" value="N">N
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <button type="submit" class="btn btn-facebook">確認匯入</button>
                        </form>
                    </div>
                    <!-- /.col-lg-6 (nested) -->
                </div>
                <!-- /.row (nested) -->
            </div>
        </div>
        <!-- /.panel -->
    </div>
</div>